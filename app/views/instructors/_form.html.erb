<!-- you have form_for an instance of a resource -->
<%= form_for @instructor do |f| %>

    <div>
        <%= f.label :first_name %>
        <%= f.text_field :first_name %>
    </div>

    <div>
        <%= f.label :last_name %>
        <%= f.text_field :last_name %>
    </div>

    <div>   
        <%= f.label :birthday %>
        <%= f.date_field :birthday %>
    </div>

    <div>
    <%= f.label :salary %>
    <%= f.number_field :salary %>
    </div>

    <div>   
        <%= f.label :highest_education %>
        <%= f.select :highest_education, options_for_select(['HS', 'College', 'Masters', 'PHD']), prompt: 'Select One' %>
    </div>

    <div>   
        <%= f.submit %>
    </div>

<% end %>