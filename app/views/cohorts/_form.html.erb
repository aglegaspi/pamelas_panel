<!-- you have form_for an instance of a resource -->
<%= form_for @cohort do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :start_date %>
        <%= f.date_field :start_date %>
    </div>

    <div>
        <%= f.label :end_date %>
        <%= f.date_field :end_date %>
    </div>

    <div>   
        <%= f.submit %>
    </div>

<% end %>