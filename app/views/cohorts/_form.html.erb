<%= form_for [@course, @cohort] do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.label :start_date %>
        <%= f.date_field :start_date %>
    </div>

    <div>
        <%= f.label :end_date %>
        <%= f.date_field :end_date %>
    </div>

    <div>   
        <%= f.label :instuctor_id %>
        <%= f.select :instructor_id, options_for_select(instructors_for_select), :prompt => 'Select' %>
    </div>

    <div>   
        <%= f.submit %>
    </div>

<% end %>